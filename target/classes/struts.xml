<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	
<struts>
	<constant name="struts.i18n.encoding" value="UTF-8" />
	<constant name="struts.action.extension" value="action" />
	<constant name="struts.serve.static.browserCache" value="false" />
	
	<package name="test" namespace="/test" extends="json-default">
		<action name="login" class="com.zwustudy.webmart.presentation.action.LoginAction">
			<result name="success" type="redirect">/success.jsp</result>
			<result name="error"   type="redirect">/error.jsp</result>
		</action>
		<action name="json" class="com.zwustudy.webmart.presentation.action.JsonAction">
			<result type="json">
				<param name="root">responseJson</param>
			</result>
		</action>
	</package>
</struts>